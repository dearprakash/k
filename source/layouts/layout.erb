<!doctype html>
<html>
  <head>
    <meta charset='utf-8'>

    <!-- Favicon -->
    <%= partial 'partials/favicon' %>
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content='IE=edge,chrome=1' http-equiv='X-UA-Compatible'>

    <!-- Viewport, set to device width -->
    <meta name='viewport' content='width=device-width,initial-scale=1'>
    <meta name="google-site-verification" content="vG3chRW_V2wGrc9qlrMT9v65BJdYBVMrjtAueAPmhD8" />
    
    <!-- Meta data -->
    <%= partial 'partials/meta' %>

    <!-- TypeKit -->
    <script src='//use.typekit.net/qec0yvt.js'></script>
    <script>try{Typekit.load();}catch(e){}</script>
    
    <%= stylesheet_link_tag 'styles' %>
  </head>
  
  <body>
    <main class='wrapper'>
      <%= partial 'partials/header' %>
      <%= yield %>
      <%= partial 'partials/footer' %>
    </main>
  </body>
</html>